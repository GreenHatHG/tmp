<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="po.Student" table="T_STUDENT">
		<composite-id name="spk" class="po.StudentPK">
			<key-property name="stuno" column="STUNO"></key-property>
			<key-property name="stuname" column="STUNAME"></key-property>
		</composite-id>		
		<property name="stusex"	column="STUSEX"/>	
	</class>
</hibernate-mapping>
